<?xml version="1.0" encoding="UTF-8"?>
<configuration>


	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>[%d{ISO8601}][%-5level][%c{3}] %m%n</pattern>
		</encoder>
	</appender>

	<root level="debug">
		<appender-ref ref="STDOUT" />
	</root>

</configuration>